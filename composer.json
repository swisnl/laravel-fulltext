{
    "name": "swisnl/laravel-fulltext",
    "type": "library",
    "description": "Fulltext indexing and searching for Laravel",
    "keywords": [
        "swisnl",
        "laravel-fulltext"
    ],
    "homepage": "https://github.com/swisnl/laravel-fulltext",
    "license": "MIT",
    "authors": [
        {
            "name": "Bj√∂rn Brala",
            "email": "bbrala@swis.nl",
            "homepage": "https://www.swis.nl/over-ons/bjorn-brala",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/console": "^10.0|^11.0",
        "illuminate/contracts": "^10.0|^11.0",
        "illuminate/database": "^10.0|^11.0",
        "illuminate/support": "^10.0|^11.0"
    },
    "require-dev": {
        "larastan/larastan": "^2.9",
        "laravel/pint": "^1.15",
        "mockery/mockery": "^1.5",
        "orchestra/testbench": "^9.0.0",
        "pestphp/pest": "^3.5",
        "pestphp/pest-plugin-laravel": "^3.0",
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpstan-deprecation-rules": "^1.2",
        "phpstan/phpstan-phpunit": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Swis\\Laravel\\Fulltext\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Swis\\Laravel\\Fulltext\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "pint --test",
        "fix-style": "pint",
        "analyse": "phpstan analyse"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Swis\\Laravel\\Fulltext\\FulltextServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
